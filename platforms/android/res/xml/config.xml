<?xml version='1.0' encoding='utf-8'?>
<widget id="com.keshavos.rainbowsandunicorns" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Rainbows &amp; Unicorns</name>
    <description>
        Test app which will reflect the app generated with the generator-angularjs-cordova generator.
        This could also be used as  a playground first where features added here will eventually make it to the generator.
    </description>
    <author email="keshav.os@gmail.com" href="http://keshavos.github.io">
        Keshav OS
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
